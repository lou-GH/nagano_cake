<div class="container px-5 px-sm-0">
  <h2>ジャンル編集</h2>

  <%= form_with model: @genre, url: admin_genres_path, method: :post do |f| %>

  <h4>ジャンル名</h4>
  <label for="inputName"></label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"ジャンル名" %>

  <%= f.submit '変更を保存' %>

  <% end %>
</div>
